              </Node>
            </RegularDetection>
            <RegularDetection MonitorTypeStateID="PoolMemberCritical">
              <Node ID="MemberCriticalFilter">
                <Node ID="DS" />
              </Node>
            </RegularDetection>
          </RegularDetections>
